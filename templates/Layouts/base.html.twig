<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}{% endblock %}</title>

        {% block css %}
        {% endblock %}

        {% block javascript %}
        {% endblock %}
    </head>
    <body>
        {% block body %}
            <div id="flash">
                {# {{ render(controller('App\\Controller\\Sandbox\\SandboxInjectionController::displayFlashAction')) }} #}
            </div>

            <div id="menu">
                {% include 'Layouts/menu.html.twig' %}
            </div>

            <div id="header">
                {% include 'Layouts/header.html.twig' %}
            </div>

            <div id="titre">
                <h1>{% block titre %}{% endblock %}</h1>
            </div>

            <div id="content">
                {% block content %}{% endblock %}
            </div>

            <div id="footer">
            {% include 'Layouts/footer.html.twig' %}
            </div>
        {% endblock %}
    </body>
</html>
