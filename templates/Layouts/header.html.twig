{% if is_granted('IS_AUTHENTICATED_REMEMBERED')%}
    {% if is_granted("ROLE_ADMIN")%}
        <img src="{{asset('Pictures/chevre_admin.png')}}" alt="image pour admin" height="256"/>
        {%else %}
            <img src="{{asset('Pictures/chevre_auth.png')}}" alt="image pour personne autenthiée" height="256"/>
